 <shortcuts>
  <programGroup defaultName="Picotask"
                location="applications"/>
  <skipIfNotSupported/>
  <shortcut
    name="picotask"
    target="$INSTALL_PATH\picotask.jar"
    commandLine=""
    workingDirectory="$INSTALL_PATH\"
    description="This starts Picotask"
    iconFile="$INSTALL_PATH\start.ico"
    iconIndex="0"
    initialState="normal"
    programGroup="yes"
    desktop="yes"
    applications="no"
    startMenu="no"
    startup="no">
  </shortcut>
</shortcuts>
